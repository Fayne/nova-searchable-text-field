{
    "name": "mobilenowgroup/searchable-text-field",
    "description": "Nova text field for searchable feature.",
    "keywords": [
        "laravel",
        "nova",
        "mobilenowgroup"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.7"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "MobileNowGroup\\SearchableTextField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MobileNowGroup\\SearchableTextField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
